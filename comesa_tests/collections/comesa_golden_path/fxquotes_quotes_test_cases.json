          },
          "body": {
            "acceptParty": true
          },
          "disabled": false,
          "tests": {
            "assertions": [
              {
                "id": 1,
                "description": "Status is 500",
                "exec": [
                  "expect(response.status).to.equal(500)"
                ]
              },
              {
                "id": 2,
                "description": "error code is 5001",
                "exec": [
                  "expect(response.body.transferState.lastError.mojaloopError.errorInformation.errorCode).to.equal('5001')"
                ]
              },
              {
                "id": 3,
                "description": "statusText should contain Generic error",
                "exec": [
                  "expect(response.body.transferState.lastError.mojaloopError.errorInformation.errorDescription).contains('Generic Payee Error')"
                ]
              }
            ]
          }
        }
      ]
    }
  ]
}