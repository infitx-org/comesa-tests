            },
            "amountType": "SEND",
            "currency": "{$inputs.P2P_CURRENCY}",
            "amount": "{$inputs.COMESA_FXQUOTES_ERROR_AMOUNT}",
            "transactionType": "TRANSFER",
            "note": "{$inputs.NOTE}",
            "skipPartyLookup": false
          },
          "params": {
            "Type": "",
