              "dateOfBirth": "{$inputs.FROM_DOB}",
              "fspId": "{$inputs.MULTI_SCHEME_PAYER_FSP_ID}"
            },
            "to": {
              "type": "CONSUMER",
              "idType": "MSISDN",
              "idValue": "{$inputs.MULTI_SCHEME_MSISDN_WITHIN_SCHEME_DFSP_MIGRATION}",
