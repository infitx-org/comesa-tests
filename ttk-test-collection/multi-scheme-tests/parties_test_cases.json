              {
                "id": 2,
                "description": "Destination FSP Id should be {$inputs.MULTI_SCHEME_PAYEE_FSP_ID}",
                "exec": [
                  "expect(response.body.getPartiesResponse.body.party.partyIdInfo.fspId).to.equal(environment.MULTI_SCHEME_PAYEE_FSP_ID)"
                ]
              }
            ]
          }
        },
        {
          "id": 3,
